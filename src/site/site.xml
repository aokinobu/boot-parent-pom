<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/DECORATION/1.3.0"         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"         xsi:schemaLocation="http://maven.apache.org/DECORATION/1.3.0 http://maven.apache.org/xsd/decoration-1.3.0.xsd"         name="${project.artifactId}">
  <skin>
    <groupId>org.apache.maven.skins</groupId>
    <artifactId>maven-fluido-skin</artifactId>
    <version>1.4</version>
  </skin>
  <poweredBy>
    <logo name="glytoucan" href="http://www.glytoucan.org" img="https://glytoucan.org/img/logo_toucan.png"/>
  </poweredBy>
  <custom>
    <fluidoSkin>
      <topBarEnabled>true</topBarEnabled>
      <sideBarEnabled>true</sideBarEnabled>
      <topBarIcon>
        <name>GlyConvert</name>
        <alt>GlyConvert</alt>
        <src>https://glytoucan.org/img/logo_toucan.png</src>
        <href>http://nexus.glycoinfo.org/content/sites/project/glyconvert/</href>
      </topBarIcon>
      <sourceLineNumbersEnabled>true</sourceLineNumbersEnabled>
      <googleSearch>
        <sitesearch/>
      </googleSearch>
      <facebookLike/>
      <googlePlusOne/>
      <twitter>
        <user>glytoucan</user>
        <showUser>true</showUser>
        <showFollowers>true</showFollowers>
      </twitter>
    </fluidoSkin>
  </custom>
  <publishDate format="dd MMM yyyy"/>
  <version position="right"/>
  <body>
    <menu name="Documentation">
      <item name="Home" href="/index.html"/>
    </menu>
    <menu name="Modules" ref="modules" />
    <menu name="Reports" ref="reports" />
    <menu name="Links" inherit="none" inheritAsRef="true">
      <item name="Github Homepage" href="http://www.github.com/glytoucan"/>
      <item name="Bitbucket Homepage" href="http://bitbucket.org/glycoSW"/>
    </menu>
  </body>
</project>
